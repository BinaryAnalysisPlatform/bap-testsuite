set test "byteweight"

foreach-binary {file "*"} "bap -d --no-objdump --no-ida" {
    "sub *print_strings" {close; wait; pass "$test $file"}
    default {
        wait;
        if [string match "*-*-gnueabihf-*" $file] {
            note "signatures do not handle gnueabihf mode"
            xfail "$test in $file"
        } else {
            fail "$test in $file"
        }
    }
}
