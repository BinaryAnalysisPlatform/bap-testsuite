set test "macho loader"

set cases {
    "ko/powerpc-xnu-be" "ref-external 45480 _abort"
    "ko/x86_64-xnu-el" "ref-external 18439 _memcmp"
}

foreach {file expected} $cases {
    spawn bap $file --dump=ogre
    expect {
        $expected {pass "$test for $file"}
        default {fail "$test for $file"}
    }
}
