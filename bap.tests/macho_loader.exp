set test "macho loader"

set files {
    "ko/powerpc-darwin-be" "ref-external 45480 _abort"
    "ko/x86_64-darwin-el" "ref-external 18439 _memcmp"
}

foreach {file expected} $files {
    spawn bap $file --dump=ogre
    expect {
        $expected {pass "$test for $file"}
        default {fail "$test for $file"}
    }
}
