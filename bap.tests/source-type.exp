set test "source-type"

set file [exec mktemp]
set syms [exec mktemp]

exec printf "(entry 0 2)" > $syms
exec printf "\\x83\\x3d\\xe9\\xb5\\x21\\x00\\x01\\xeb\\xf7" > $file

match $file asm "--loader=raw --raw-arch=x86" {
    "cmpl" {pass "$test"}
    default {fail "$test"}
}

exec rm $file
exec rm $syms
