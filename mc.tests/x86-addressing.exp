    {0x36,0x89,0x44,0x95,0x00} {movl %eax, %ss:\(%ebp,%edx,4\)} {.*\[EBP \+ \(EDX << 2\), el\].*}
    {0x36,0x89,0x44,0xd5,0x00} {movl %eax, %ss:\(%ebp,%edx,8\)} {.*\[EBP \+ \(EDX << 3\), el\].*}
    {0x36,0x89,0x45,0x04} {movl %eax, %ss:0x4\(%ebp\)} {.*\[EBP \+ 4, el\].*}
    {0x36,0x89,0x44,0x15,0x04} {movl %eax, %ss:0x4\(%ebp,%edx\)} {.*\[EBP \+ EDX \+ 4, el\].*}
    {0x36,0x89,0x44,0x55,0x04} {movl %eax, %ss:0x4\(%ebp,%edx,2\)} {.*\[EBP \+ \(EDX << 1\) \+ 4, el\].*}
    {0x36,0x89,0x44,0x95,0x04} {movl %eax, %ss:0x4\(%ebp,%edx,4\)} {.*\[EBP \+ \(EDX << 2\) \+ 4, el\].*}
    {0x36,0x89,0x44,0xd5,0x04} {movl %eax, %ss:0x4\(%ebp,%edx,8\)} {.*\[EBP \+ \(EDX << 3\) \+ 4, el\].*}
    {0x36,0x89,0x3c,0x05,0x00,0x00,0x00,0x00} {movl %edi, %ss:\(,%eax\)} {.*\[EAX, el\].*}
    {0x36,0x89,0x3c,0x45,0x00,0x00,0x00,0x00} {movl %edi, %ss:\(,%eax,2\)} {.*\[EAX << 1, el\].*}
    {0x36,0x89,0x3c,0x85,0x00,0x00,0x00,0x00} {movl %edi, %ss:\(,%eax,4\)} {.*\[EAX << 2, el\].*}
    {0x36,0x89,0x3c,0xc5,0x00,0x00,0x00,0x00} {movl %edi, %ss:\(,%eax,8\)} {.*\[EAX << 3, el\].*}
    {0x36,0x89,0x3c,0x05,0x04,0x00,0x00,0x00} {movl %edi, %ss:0x4\(,%eax\)} {.*\[EAX \+ 4, el\].*}
    {0x36,0x89,0x3c,0x45,0x04,0x00,0x00,0x00} {movl %edi, %ss:0x4\(,%eax,2\)} {.*\[\(EAX << 1\) \+ 4, el\].*}
    {0x36,0x89,0x3c,0x85,0x04,0x00,0x00,0x00} {movl %edi, %ss:0x4\(,%eax,4\)} {.*\[\(EAX << 2\) \+ 4, el\].*}
    {0x36,0x89,0x3c,0xc5,0x04,0x00,0x00,0x00} {movl %edi, %ss:0x4\(,%eax,8\)} {.*\[\(EAX << 3\) \+ 4, el\].*}
}

proc run {arch suite} {
    foreach {code asm bil} $suite {
        set test "$arch.addressing.\[$asm\] -> $code"
        set opts [subst {
            "--arch=$arch"
            "--show-insn=asm"
            "--x86-lifter=modern"
            "--show-bil=pretty"
        }]
        set opts [join $opts " "]
        match $code $opts {
            "$asm.$bil" {pass $test }
            default { fail "$test" }
        }

    }
}

foreach arch  [array names suite] {
    run $arch $suite($arch)
}

unset -nocomplain suite
