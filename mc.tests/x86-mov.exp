    {0xc7,0x00,0x04,0x00,0x00,0x00} {movl \$0x4, \(%rax\)} {mem := mem with \[RAX, el\]:u32 <- 4}
    {0x48,0xc7,0x00,0x04,0x00,0x00,0x00} {movq \$0x4, \(%rax\)} {mem := mem with \[RAX, el\]:u64 <- 4}

}

