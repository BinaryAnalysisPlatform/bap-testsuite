    {0x88,0xe3} {movb %ah, %bl} {RBX := high:56\[RBX\].extract:15:8\[RAX\]}
    {0x88,0x03} {movb %al, \(%rbx\)} {mem := mem with \[RBX\] <- low:8\[RAX\]}
