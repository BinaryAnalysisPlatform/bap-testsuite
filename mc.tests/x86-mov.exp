proc run {arch suite} {
    foreach {code asm bil} $suite {
        set test "$arch.mov.\[$asm\] -> $code"
        set opts [subst {
            "--arch=$arch"
            "--show-insn=asm"
            "--x86-lifter=modern"
            "--show-bil=pretty"
        }]
        set opts [join $opts " "]

        match $code $opts {
            "$asm.*$bil" {pass $test }
            default { fail "$test" }
        }
    }
}

foreach arch  [array names suite] {
    run $arch $suite($arch)
}

unset -nocomplain suite
