# throw garbage into x86 disassembler and get nice output
set test "x86-torture"

set suite {
    {\x89\xc3\x00} {89 C3(00)}
    {\x89\xc3\x00\x89\xc3} {89 C3(00 89)C3}
    {\x00} {(00)}
}


foreach {input output} $suite {
    set test "$test<$input"
    spawn bap-mc --arch=x86 "$input"
    expect {
        $output {pass $test}
        default {fail $test}
    }
}
